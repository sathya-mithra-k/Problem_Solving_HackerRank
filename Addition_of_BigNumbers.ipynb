   "source": [
    "def aVeryBigSum(a):\n",
    "    sums = 0\n",
    "    for i in range(len(a)):\n",
    "        sums = sums +a[i]\n",
    "        i+=1\n",
    "    return sums\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    N = int(input().strip())\n",
    "    A = list(map(int,input().strip().split()))\n",
    "    result = aVeryBigSum(A)\n",
    "    print(result)\n"
   ]
