   "source": [
    "def miniMaxSum(arr):\n",
    "    l1=[]\n",
    "    for i in arr:\n",
    "        x=-i\n",
    "        for j in arr:\n",
    "            x+=j\n",
    "        l1.append(x)\n",
    "    print(min(l1),max(l1))"
   ]
  },
