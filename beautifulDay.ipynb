    "def beautifulDays(i, j, k):\n",
    "    count = 0\n",
    "    for a in range(i,j+1):\n",
    "        b = str(a)\n",
    "        c = int(b[::-1])\n",
    "        x = abs(a-c)\n",
    "        if x % k == 0:\n",
    "            count +=1\n",
    "    return count    \n",
    "\n",
    "beautifulDays, 23, 6)"
   ]
